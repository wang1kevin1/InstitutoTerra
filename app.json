{
  "expo": {
    "name": "Refloresta",
    "slug": "Refloresta",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "splash": {
        "image": "./src/assets/splash/splash-hdpi.png",
        "resizeMode": "cover"
      },
      "bundleIdentifier": "app.refloresta"
    },
    "android": {
      "splash": {
        "ldpi": "./src/assets/splash/splash-hdpi.png",
        "mdpi": "./src/assets/splash/splash-hdpi.png",
        "hdpi": "./src/assets/splash/splash-hdpi.png",
        "xhdpi": "./src/assets/splash/splash-hdpi.png",
        "xxhdpi": "./src/assets/splash/splash-hdpi.png",
        "xxxhdpi": "./src/assets/splash/splash-hdpi.png",
        "resizeMode": "cover"
      },
      "package": "app.refloresta"
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "description": "An app promoting the planting of trees to offset carbon emissions. http://www.institutoterra.org/eng/",
    "githubUrl": "https://github.com/wang1kevin1/InstitutoTerra.git"
  }
}
