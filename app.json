{
  "expo": {
    "name": "ZeroCarbon",
    "slug": "InstitutoTerra",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "splash": {
        "image": "./src/assets/splash/splash-xhdpi.png",
        "resizeMode": "cover"
      },
      "bundleIdentifier": "org.institutoterra.zerocarbon"
    },
    "android": {
      "splash": {
        "ldpi": "./src/assets/splash/splash-mdpi.png",
        "mdpi": "./src/assets/splash/splash-mdpi.png",
        "hdpi": "./src/assets/splash/splash-xhdpi.png",
        "xhdpi": "./src/assets/splash/splash-xhdpi.png",
        "xxhdpi": "./src/assets/splash/splash-xxxhdpi.png",
        "xxxhdpi": "./src/assets/splash/splash-xxxhdpi.png",
        "resizeMode": "cover"
      },
      "package": "org.institutoterra.zerocarbon"
    }
  }
}
